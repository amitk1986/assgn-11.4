<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-4.2.xsd">


<bean id="animal" class="com.diablo.assgn114.Dog">
<constructor-arg value="12"></constructor-arg>
<constructor-arg value="bark"></constructor-arg>
<constructor-arg ref="leg"></constructor-arg>
</bean>
<bean id="leg" class="com.diablo.assgn114.Legs">
<constructor-arg value="12"></constructor-arg>
<constructor-arg value="45"></constructor-arg>
</bean>

</beans>